{
  "releases": [
    {
      "name": "\($RELEASE_NAME)",
      "chart": "../cd/helm/\($CHART_NAME)/",
      "namespace": "\($NAMESPACE)",
      "atomic": true,
      "wait": true,
      "values": [
        "../cd/helm/\($FOLDER_NETWORK_APP)/values.yaml",
        {
          "env": {
            "fogusHostname": "\($HOSTNAME_NETAPP)",
            "environment": "\($DEPLOYMENT)"
          }
        },
        {
          "fe": {
            "replicas": "\($APP_REPLICAS)"
          }
        }
      ]
    }
  ]
}